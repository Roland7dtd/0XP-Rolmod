<configs>
	<!-- This will log as a WRN for not able to remove -->
	<!-- This is done on purpose to have a log entry showing this file was read -->
	<remove xpath="/Successfully_Loaded[@modname='RELXIX_UI_ZeeNoPlayerXPBar'][@version='1.0.3'][@filename='\Config\XUi\windows.xml']" />

<!--
			<sprite depth="2" pos="0,5" height="12" type="sliced" color="[black]" />
			<sprite depth="3" pos="3,3" height="8" width="598" color="[lightGrey]" foregroundlayer="true" type="sliced" />
			<sprite depth="4" pos="3,3" height="8" width="598" color="49,91,203" sprite="menu_empty3px" foregroundlayer="true" type="filled" fill="{xp}" />
-->
	<!-- Removes numbers XP bar from toolbelt. -->  <!-- We use multiple identifiers to make sure removing the correct element since the ones we are interested in have no name. -->
	<remove xpath="/windows/window[@name='windowToolbelt']/rect/sprite[@depth='2'][@pos='0,5'][@height='12'][@type='sliced'][@color='[black]']" />
	<remove xpath="/windows/window[@name='windowToolbelt']/rect/sprite[@depth='3'][@pos='3,3'][@height='8'][@type='sliced'][@color='[lightGrey]']" />
	<remove xpath="/windows/window[@name='windowToolbelt']/rect/sprite[@depth='4'][@pos='3,3'][@height='8'][@type='filled'][@color='49,91,203']" />
</configs>
